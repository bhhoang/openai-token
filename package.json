{
  "name": "bhhoang-gpt-token",
  "version": "1.0.2",
  "description": "OpenAI Authentication Library for ChatGPT.",
  "type": "module",
  "source": "./src/index.ts",
  "types": "./build/index.d.ts",
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "import": "./build/index.js",
      "types": "./build/index.d.ts",
      "default": "./build/index.cjs",
      "require": "./build/index.cjs"
    }
  },
  "engines": {
    "node": ">=14.18.0 <15.0.0 || >=16.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "del build",
    "prebuild": "npm run clean",
    "predev": "npm run clean"
  },
  "keywords": [
    "openai",
    "auth",
    "chatgpt",
    "gpt3",
    "gpt-3"
  ],
  "author": "bincooo",
  "repository": "bhhoang/openai-token",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.3",
    "axios-cookiejar-support": "^4.0.6",
    "http-cookie-agent": "^5.0.2",
    "http-proxy-agent": "^5.0.0",
    "https-proxy-agent": "^5.0.1",
    "tough-cookie": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^14.18.36",
    "del-cli": "^5.0.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  }
}
